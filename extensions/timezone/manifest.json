{
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "match_about_blank": true,
            "all_frames": true,
            "run_at": "document_start",
            "js": [
                "inject.js"
            ]
        }
    ],
    "manifest_version": 2,
    "name": "Proxy Manager Timezone",
    "permissions": [
        "storage",
        "*://*/*",
        "webNavigation"
    ],
    "version": "1.410.597"
}