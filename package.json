{
  "name": "luminati-proxy",
  "version": "0.10.0",
  "description": "A configurable local proxy for luminati.io",
  "homepage": "https://luminati.io/",
  "main": "index.js",
  "engines": {
    "node": ">=4.9&&<7"
  },
  "recomendedNode": ">=6",
  "scripts": {
    "start": "bin/luminati.js",
    "electron": "electron .",
    "test": "./node_modules/mocha/bin/mocha",
    "test-win": "node node_modules/mocha/bin/mocha"
  },
  "keywords": [
    "luminati",
    "proxy",
    "hola"
  ],
  "author": "Gilad Novik <gilad@hola.org>",
  "license": "ISC",
  "preferGlobal": "true",
  "bin": {
    "luminati-proxy": "bin/luminati.js",
    "luminati": "bin/luminati.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hola/luminati-proxy.git"
  },
  "bugs": {
    "email": "gilad@hola.org"
  },
  "build": {
    "npmRebuild": false,
    "appId": "luminati.proxy_manager",
    "squirrelWindows": {
      "remoteReleases": "http://cdn.luminati.io/static/lpm"
    },
    "win": {
      "iconUrl": "http://luminati.io/favicon.ico"
    }
  },
  "dependencies": {
    "angular": "^1.6.0",
    "angular-sanitize": "^1.6.0",
    "angular-ui-bootstrap": "^2.3.1",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "bootstrap-datepicker": "^1.6.4",
    "check-node-version": "^1.1.2",
    "codemirror": "^5.21.0",
    "country-language": "^0.1.7",
    "es6-shim": "^0.35.3",
    "express": "^4.14.0",
    "flag-icon-css": "^2.8.0",
    "font-awesome": "^4.7.0",
    "http-shutdown": "^1.1.0",
    "hutil": "^0.1.9",
    "jquery": "^3.1.1",
    "json-stable-stringify": "^1.0.1",
    "json2csv": "^3.7.3",
    "lodash": "^4.17.2",
    "moment": "^2.17.1",
    "mysql": "^2.13.0",
    "node-forge": "^0.6.46",
    "notosans-fontface": "^1.0.1",
    "pm2": "^2.2.2",
    "request": "^2.79.0",
    "request-stats": "^2.0.1",
    "require-css": "^0.1.8",
    "requirejs": "^2.3.2",
    "semver": "^5.3.0",
    "sequelize": "^3.30.2",
    "socksv5": "0.0.6",
    "sqlite3": "3.1.8",
    "sudo-prompt": "^6.2.1",
    "swagger-ui": "^2.2.8",
    "universal-analytics": "^0.4.9",
    "winston": "^2.3.0",
    "yargs": "^6.5.0"
  },
  "devDependencies": {
    "electron": "^1.4.1",
    "electron-rebuild": "^1.5.7",
    "electron-builder": "^13.9.0",
    "electron-builder-squirrel-windows": "^13.6.0",
    "electron-squirrel-startup": "^1.0.0",
    "mocha": "^3.2.0"
  },
  "optionalDependencies": {
    "phantomjs-prebuilt": "^2.1.14",
    "webshot": "^0.18.0"
  }
}
