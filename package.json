      "provider": "generic",
      "url": "http://luminati.io/static/lpm"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "tar.gz"
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
