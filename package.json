  "build": {
    "appId": "luminati.proxy_manager",
    "asar": false,
    "artifactName": "${name}-${version}.${ext}",
    "directories": {
      "output": "d"
    },
    "files": [
      "!publish*.js*",
      "!**/.jakecache${/*}",
      "!**/test_ln",
      "!**/xvfs_ln",
      "!**/builddir_ln",
      "!build.lock",
      "!luminati-proxy-*",
      "!latest.yml",
      "!deploy${/*}"
    ],
    "npmRebuild": false,
    "nsis": {
      "artifactName": "${name}-${version}-setup.${ext}",
      "warningsAsErrors": false
    },
    "productName": "Luminati Proxy Manager",
    "publish": {
      "provider": "generic",
      "url": "http://luminati.io/static/lpm"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "tar.gz"
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  },
