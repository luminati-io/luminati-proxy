      "!publish*.js*",
      "!**/.jakecache${/*}",
      "!**/test_ln",
      "!**/xvfs_ln",
      "!**/builddir_ln",
      "!build.lock",
      "!luminati-proxy-*",
      "!latest.yml",
      "!deploy${/*}"
